<?php

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help()
*/

 function hello_world($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.hello_world':
      $output = '';
      $output .= '<h3>' . ('About') . '</h3>';
      $output .= '<p>' . ('This is Drupal module') .'</p>';
      return $output;
      default: return;
  }
 }